<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GraphicController.PurchasedProductsController">
   <children>
      <Label layoutX="14.0" layoutY="63.0" text="Buy Log: ">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Label layoutX="290.0" layoutY="14.0" text="Purchased Products">
         <font>
            <Font name="System Bold Italic" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="buyLogTableView" layoutX="73.0" layoutY="101.0" onMouseClicked="#userClickedOnTable" prefHeight="356.0" prefWidth="654.0">
         <columns>
            <TableColumn fx:id="productColumn" prefWidth="223.0" text="Product" />
            <TableColumn fx:id="priceColumn" minWidth="0.0" prefWidth="149.0" text="Amount" />
            <TableColumn fx:id="discountColumn" prefWidth="298.0" text="Price" />
         </columns>
      </TableView>
      <Button fx:id="showLogButton" layoutX="676.0" layoutY="479.0" mnemonicParsing="false" onAction="#scoringOnClick" text="Show Log" />
   </children>
</AnchorPane>
